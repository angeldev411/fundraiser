{
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "indent": [1, 2],
        "linebreak-style": [2, "unix"],
        //"quotes": [2, "single"],
        "semi": [1, "always"],
        "brace-style": [2, "1tbs", { "allowSingleLine": true } ],
        "array-bracket-spacing": [2, "never"],
        "camelcase": [1, {"properties": "always"}],
        "keyword-spacing": [1],
        "eol-last": [2],
        "comma-dangle": [1],
        "no-trailing-spaces": [2],
        "no-console": ["error", { "allow": ["warn", "error"] }]
    },
    "globals": {
      "moment": true //
      
    }
}